  "author": "Mario Casciaro <m@mario.fyi>",
  "homepage": "https://github.com/mariocasciaro/object-path-immutable",
  "repository": {
    "type": "git",
    "url": "git://github.com/mariocasciaro/object-path-immutable.git"
  },
